{
    "name": "softspring/response-headers",
    "description": "This component provides response headers configuration for Symfony projects",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "symfony/event-dispatcher": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "symfony/expression-language": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\ResponseHeaders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\ResponseHeaders\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
