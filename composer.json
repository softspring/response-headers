{
    "name": "softspring/response-headers",
    "description": "This component provides response headers configuration for Symfony projects",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/event-dispatcher": "^5.4|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "symfony/expression-language": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\ResponseHeaders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\ResponseHeaders\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev"
        }
    }
}
